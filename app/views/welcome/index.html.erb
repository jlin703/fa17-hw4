<h1>Cats</h1>
<% @cats.each do |cat| %>
  <%= cat.name %><br/>
<% end %>

<h1>Todos</h1>
<% @todos.each do |todo| %>
  Task: <%=todo.tasks%> --- Finished? <%= todo.finished%><br/>
<% end %>

<%= link_to("Create Task", new_todo_path) %>

<h1>Users</h1>
<% @users.each do |user| %>
  <%= user.username %><br/>
<% end %>
